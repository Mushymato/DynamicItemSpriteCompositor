{
  "$schema": "https://smapi.io/schemas/content-patcher.json",
  "Format": "2.8.0",
  "Changes": [
    {
      "Action": "Load",
      "Target": "{{ModId}}/jars, {{ModId}}/jars_noclr, {{ModId}}/keggy, {{ModId}}/keggy_inv",
      "FromFile": "assets/{{TargetWithoutPath}}.png"
    },
    {
      "Action": "EditData",
      "Target": "mushymato.DISCO/Data/{{ModId}}",
      "Entries": {
        "Juicey": {
          "TypeIdentifier": "(O)",
          "LocalItemId": "350",
          "SourceTextures": "{{ModId}}/jars",
          "Rules": [
            {
              "RequiredColor": "color_orange",
              "SpriteIndexList": 0
            },
            {
              "RequiredColor": "color_yellow",
              "SpriteIndexList": 2
            }
          ]
        },
        "JuiceyNoClr": {
          "TypeIdentifier": "(O)",
          "LocalItemId": "350",
          "SourceSpritePerIndex": 1,
          "SourceTextures": "{{ModId}}/jars_noclr",
          "Rules": [
            {
              "RequiredColor": "color_orange",
              "SpriteIndexList": 0
            },
            {
              "RequiredColor": "color_yellow",
              "SpriteIndexList": 1
            },
            {
              "RequiredColor": "color_green",
              "SpriteIndexList": 2
            }
          ]
        },
        "CaskTest": {
          "TypeIdentifier": "(O)",
          "LocalItemId": "348",
          "SourceSpritePerIndex": 1,
          "SourceTextures": "{{ModId}}/jars_noclr",
          "Rules": [
            {
              "RequiredContextTags": "quality_silver",
              "SpriteIndexList": 3
            }
          ]
        },
        "Keggy": {
          "TypeIdentifier": "(BC)",
          "LocalItemId": "12",
          "SourceTextures": "{{ModId}}/keggy,{{ModId}}/keggy_inv",
          "Rules": [
            {
              "SpriteIndexList": "0,1,2",
              "IncludeDefaultSpriteIndex": true,
            },
          ]
        }
      }
    }
  ]
}